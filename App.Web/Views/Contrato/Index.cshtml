@model IEnumerable<App.Model.Contrato.Contrato>


<h3>Contratos</h3>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.RUT)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Pl_UndDes)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NombreJefeSolicitante)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NombreCompleto)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.RUT)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Pl_UndDes)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NombreJefeSolicitante)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NombreCompleto)
        </td>
        <td>
            @Html.ActionLink("Detalles", "View", new { id = item.ContratoId })
        </td>
    </tr>
}
</table>
