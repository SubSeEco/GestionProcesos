@model App.Model.CDP.CDP

<div class="row">
    <div class="col s12 m6 l3">
        <div class="card">
            <div class="card-content">
                <span class="card-title">CDP</span>
                <div class="row">

                    <div class="input-field col s12 m10">
                        <strong>@Html.DisplayNameFor(m => Model.Solicitante)</strong><br />
                        @Html.DisplayFor(m => Model.Solicitante)
                    </div>
                    <div class="input-field col s12 m10">
                        <strong>@Html.DisplayNameFor(m => Model.Solicitante)</strong><br />
                        @Html.DisplayFor(m => Model.Solicitante)
                    </div>
                    <div class="input-field col s12 m10">
                        <strong>@Html.DisplayNameFor(m => Model.Institucion.Nombre)</strong><br />
                        @Html.DisplayFor(m => Model.Institucion.Nombre)
                    </div>
                    <div class="input-field col s12 m10">
                        <strong>@Html.DisplayNameFor(m => Model.FechaSolicitud)</strong><br />
                        @Html.DisplayFor(m => Model.FechaSolicitud)
                    </div>
                    <div class="input-field col s12 m10">
                        <strong>@Html.DisplayNameFor(m => Model.Region.Nombre)</strong><br />
                        @Html.DisplayFor(m => Model.Region.Nombre)
                    </div>
                    <div class="input-field col s12 m10">
                        <strong>@Html.DisplayNameFor(m => Model.Observacion)</strong><br />
                        @Html.DisplayFor(m => Model.Observacion)
                    </div>

                </div>
            </div>
        </div>
    </div>
    <div class="col s12 m6 l3">
        @Html.Action("Documents", "Workflow", new { Model.WorkflowId })
        @Html.Action("History", "Workflow", new { Model.WorkflowId })
    </div>
</div>
