                                            <span>@Html.LabelFor(m => Model.Pasajes)</span>
                                        </label>
                                        <div class="section"></div>
                                    </div>
                                </div>
                                <div class="input-field col s12 m10 l10">
                                    <label>
                                        @Html.CheckBoxFor(m => Model.SolicitaViatico)
                                        <span>@Html.LabelFor(m => Model.SolicitaViatico)</span>
                                    </label>
                                    <div class="section"></div>
                                </div>
                                <div class="input-field col s12 m10 l10">
                                    <label>
                                        @Html.CheckBoxFor(m => Model.ReqPasajeAereo)
                                        <span>@Html.LabelFor(m => Model.ReqPasajeAereo)</span>
                                    </label>
                                    <div class="section"></div>
                                </div>
                                <div class="input-field col s12 m10 l10">
                                    <label>
                                        @Html.CheckBoxFor(m => Model.ReqPasajeTerrestre)
                                        <span>@Html.LabelFor(m => Model.ReqPasajeTerrestre)</span>
                                    </label>
                                    <div class="section"></div>
                                </div>
                                <div id="divReembolso">
                                    <div class="input-field col s12 m4 l3">
                                        <label>
                                            @Html.CheckBoxFor(m => Model.SolicitaReembolso)
                                            <span>@Html.LabelFor(m => Model.SolicitaReembolso)</span>
                                        </label>
                                        <div class="section"></div>
                                    </div>
                                </div>
                                <div class="input-field col s12 m4 l3">
                                    <label>
                                        @Html.CheckBoxFor(m => Model.Vehiculo)
                                        <span>@Html.LabelFor(m => Model.Vehiculo)</span>
                                    </label>
                                    <div class="section"></div>
                                </div>
                                <div id="auto">
                                    <div class="input-field col s12 m6 l10">
                                        @Html.LabelFor(m => Model.TipoVehiculoId)
                                        @Html.DropDownListFor(m => Model.TipoVehiculoId, null, "Seleccione...")
                                        @Html.ValidationMessageFor(m => Model.TipoVehiculoId, "", new { @class = "red-text" })
                                    </div>
                                    <div class="input-field col s12 m2 l10">
                                        @Html.LabelFor(m => Model.PlacaVehiculo)
                                        @Html.TextBoxFor(m => Model.PlacaVehiculo)
                                        @Html.ValidationMessageFor(m => Model.PlacaVehiculo, "", new { @class = "red-text" })
                                    </div>
                                </div>
                            </div>
                            @*<div class="section"></div>*@
                            <h3><span style="font-weight: bold">III. Datos del Cometido: </span></h3>
                            <div class="row">
                                <div class="input-field col s12 m10 l10">
                                    @Html.EditorFor(m => Model.NombreCometido)
                                    @Html.LabelFor(m => Model.NombreCometido)
                                    @Html.ValidationMessageFor(m => Model.NombreCometido, "", new { @class = "red-text" })
                                </div>
                                <div class="input-field col s12 m10 l10">
                                    @Html.EditorFor(m => Model.CometidoDescripcion)
                                    @Html.LabelFor(m => Model.CometidoDescripcion)
                                    @Html.ValidationMessageFor(m => Model.CometidoDescripcion, "", new { @class = "red-text" })
                                </div>
                                <div class="input-field col s12 m10 l10">
                                    <span class="card-title">Destinos</span>
                                </div>

                                @if (Model.Destinos.Count > 0)
                                {
                                    <table class="table">
                                        <tr>
                                            <th>
                                                @Html.DisplayNameFor(model => model.Destinos.FirstOrDefault().RegionDescripcion)
                                            </th>
                                            <th>
                                                @Html.DisplayNameFor(model => model.Destinos.FirstOrDefault().ComunaDescripcion)
                                            </th>
                                            <th>
                                                @Html.DisplayNameFor(model => model.Destinos.FirstOrDefault().FechaInicio)
                                            </th>
                                            <th>
                                                @Html.DisplayNameFor(model => model.Destinos.FirstOrDefault().FechaHasta)
                                            </th>
                                            <th>
                                                @Html.DisplayNameFor(m => Model.Destinos.FirstOrDefault().Dias100)
                                            </th>
                                            <th>
                                                @Html.DisplayNameFor(m => Model.Destinos.FirstOrDefault().Dias60)
                                            </th>
                                            <th>
                                                @Html.DisplayNameFor(m => Model.Destinos.FirstOrDefault().Dias40)
                                            </th>
                                            <th>
                                                @Html.DisplayNameFor(m => Model.Destinos.FirstOrDefault().Dias00)
                                            </th>
                                            <th>
                                                @Html.DisplayNameFor(m => Model.Destinos.FirstOrDefault().Dias50)
                                            </th>
                                            <th>
                                                @Html.DisplayNameFor(m => Model.Destinos.FirstOrDefault().Total)
                                            </th>
                                            <th></th>
                                        </tr>
                                        @for (int i = 0; i < Model.Destinos.ToArray().Length; i++)
                                        {
                                            <tr>
                                                <td>
                                                    @Html.DisplayFor(m => Model.Destinos.ToArray()[i].RegionDescripcion)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(m => Model.Destinos.ToArray()[i].ComunaDescripcion)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(model => Model.Destinos.ToArray()[i].FechaInicio)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(model => Model.Destinos.ToArray()[i].FechaHasta)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(m => Model.Destinos.ToArray()[i].Dias100)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(m => Model.Destinos.ToArray()[i].Dias60)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(m => Model.Destinos.ToArray()[i].Dias40)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(m => Model.Destinos.ToArray()[i].Dias00)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(m => Model.Destinos.ToArray()[i].Dias50)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(m => Model.Destinos.ToArray()[i].Total)
                                                </td>
                                                <td>
                                                    @*@Html.ActionLink("Editar", "Edit", "Destinos", new { id = Model.Destinos.ToArray()[i].DestinoId }, null)*@
                                                    <a href='@Url.Action("Edit", "Destinos", new { id = Model.Destinos.ToArray()[i].DestinoId })'><img src="~/Content/images/icon-edit.png"></img></a>
                                                </td>
                                                <td>
                                                    @*@Html.ActionLink("Borrar", "Delete", "Destinos", new { id = Model.Destinos.ToArray()[i].DestinoId }, null)*@
                                                    <a href='@Url.Action("Delete", "Destinos", new { id = Model.Destinos.ToArray()[i].DestinoId })'><img src="~/Content/images/icon-borrar.png"></img></a>
                                                </td>
                                            </tr>
                                        }
                                    </table>

                                }
                                <div class="section"></div>
                                @*@Html.ActionLink("Agregar Destino", "Create", "Destinos", new { id = Model.CometidoId }, null)*@
                                <a href='@Url.Action("Create","Destinos", new { id = Model.CometidoId })' class="btn-floating modal-trigger btn-large blue tooltipped" data-position="right" data-tooltip="Agregar destinos"><i class="large material-icons">add</i></a>
                            </div>

                            <!--se agregan datos de pasajes si se solicitan-->
                            @if (Model.ReqPasajeAereo == true)
                            {
                                <h3><span style="font-weight: bold;height:inherit">IV. Datos del Pasaje: </span></h3>
                                <div class="row">
                                    @Html.Partial("_DestinosPasajesPartial", (App.Model.Pasajes.DestinosPasajes)ViewBag.Pasaje)

                                    @if (ViewBag.DestinosPasajes != null)
                                    {
                                        <table class="table">
                                            <thead>
                                                <tr>
                                                    <th>Región de origen</th>
                                                    <th>Fecha de origen</th>
