                                    @Html.DisplayFor(m => Model.EstamentoDescripcion)
                                    @Html.LabelFor(m => Model.EstamentoDescripcion)
                                    @Html.ValidationMessageFor(m => Model.EstamentoDescripcion, "", new { @class = "red-text" })
                                </div>
                                <div class="input-field col s12 m6 l3">
