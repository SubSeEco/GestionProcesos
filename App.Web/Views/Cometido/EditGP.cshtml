@model App.Model.Cometido.Cometido


@Html.Action("Header", "Workflow", new { Model.WorkflowId })
<div class="container">

    @Html.Action("procesState", "HorasExtras", new { Model.WorkflowId })


    <div id="card-alert" class="card blue-grey lighten-1">
        <div class="card-content white-text">
            <span class="card-title white-text darken-1"><i class="mdi-social-notifications"></i>Atencion.!</span>
            @if (Model.Destinos.Any())
            {
                if ((Model.Destinos.FirstOrDefault().FechaInicio - DateTime.Now).Days < 0)
                {
                    <p>
                        El cometido se realizó hace @((DateTime.Now - Model.Destinos.FirstOrDefault().FechaInicio).Days) dias.
                    </p>
                }
                else
                {
                    <p>
                        Faltan  @((Model.Destinos.FirstOrDefault().FechaInicio - DateTime.Now).Days + 1) días para el viaje. Recuerde que el acto administrativo debe tramitarse al menos 20 días corridos previo a su inicio.
                    </p>
                }
            }
            else
            {
                <p>No existen destinos asociados al cometido</p>
            }
        </div>
    </div>

    <div class="row">
        <div class="col s12 m10 l8">
            @Html.Partial("_Message")
            @using (Html.BeginForm("EditGP", null, FormMethod.Post, new { enctype = "multipart/form-data" }))
            {
                @Html.HiddenFor(q => Model.WorkflowId)
                @Html.HiddenFor(q => Model.ProcesoId)
                @Html.HiddenFor(q => Model.CometidoId)
                @Html.AntiForgeryToken()

                @Html.HiddenFor(q => Model.FechaSolicitud)
                @Html.HiddenFor(q => Model.UnidadDescripcion)
                @Html.HiddenFor(q => Model.NombreId)
                @Html.HiddenFor(q => Model.Nombre)
                @Html.HiddenFor(q => Model.Rut)
                @Html.HiddenFor(q => Model.DV)
                @Html.HiddenFor(q => Model.Jefatura)
                @Html.HiddenFor(q => Model.CargoDescripcion)
                @Html.HiddenFor(q => Model.CalidadDescripcion)
                @Html.HiddenFor(q => Model.GradoDescripcion)
                @Html.HiddenFor(q => Model.EstamentoDescripcion)
                @Html.HiddenFor(q => Model.ConglomeradoDescripcion)
                @Html.HiddenFor(q => Model.ProgramaDescripcion)
                @Html.HiddenFor(q => Model.IdCargo)
                @Html.HiddenFor(q => Model.IdUnidad)
                @Html.HiddenFor(q => Model.IdCargo)
                @Html.HiddenFor(q => Model.IdCalidad)
                @Html.HiddenFor(q => Model.IdGrado)
                @Html.HiddenFor(q => Model.IdEstamento)
                @Html.HiddenFor(q => Model.FinanciaOrganismo)
                @Html.HiddenFor(q => Model.ReqPasajeAereo)
                @Html.HiddenFor(q => Model.ReqPasajeTerrestre)
                @Html.HiddenFor(q => Model.Pasajes)
                @Html.HiddenFor(q => Model.SolicitaViatico)
                @Html.HiddenFor(q => Model.Vehiculo)
                @Html.HiddenFor(q => Model.TipoVehiculoId)
                @Html.HiddenFor(q => Model.TipoVehiculoDescripcion)
                @Html.HiddenFor(q => Model.PlacaVehiculo)
                @Html.HiddenFor(q => Model.IdEscalafon)
                @Html.HiddenFor(q => Model.EscalafonDescripcion)
                @Html.HiddenFor(q => Model.Folio)
                @Html.HiddenFor(q => Model.FechaResolucion)
                @Html.HiddenFor(q => Model.TipoActoAdministrativo)

                <div class="card">
                    <div class="card-content">
                        @*<span class="card-title">Cometidos</span>*@
                        <div class="row">
                            <div class="input-field col s12 m6 l6">
                                <strong>
                                    @Html.DisplayNameFor(m => Model.FechaSolicitud)
                                </strong>
                                @Html.DisplayFor(m => Model.FechaSolicitud)

                            </div>
                            <div class="input-field col s12 m10 l6">
                                <strong>@Html.DisplayNameFor(m => Model.CometidoId)</strong>
                                @Html.DisplayFor(m => Model.CometidoId)
                            </div>
                        </div>
                        <div id="jQuery_accordion">
                            <h3><span style="font-weight: bold">I. Datos del Funcionario: </span></h3>
                            <div class="row">
                                <div class="input-field col s12 m10 l10">
                                    <strong>@Html.DisplayNameFor(m => Model.UnidadDescripcion)</strong>
                                    @Html.DisplayFor(m => Model.UnidadDescripcion)
                                </div>
                                <div class="input-field col s12 m10 l10">
                                    <strong>@Html.DisplayNameFor(m => Model.Nombre)</strong>
                                    @Html.DisplayFor(m => Model.Nombre)
                                </div>
                                <div class="input-field col s12 m6 l8">
                                    <strong>@Html.DisplayNameFor(m => Model.Rut)</strong>
                                    @Html.DisplayFor(m => Model.Rut)
                                </div>
