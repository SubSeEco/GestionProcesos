                                    <strong>@Html.DisplayNameFor(m => Model.CargoDescripcion)</strong>
                                    @Html.DisplayFor(m => Model.CargoDescripcion)
                                </div>
                                <div class="input-field col s12 m6 l3">
                                    <strong>@Html.DisplayNameFor(m => Model.CalidadDescripcion)</strong>
                                    @Html.DisplayFor(m => Model.CalidadDescripcion)
                                </div>
                                <div class="input-field col s12 m6 l3">
                                    <strong>@Html.DisplayNameFor(m => Model.GradoDescripcion)</strong>
