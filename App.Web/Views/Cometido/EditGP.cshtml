                                        <th>
                                            @Html.DisplayNameFor(model => model.Destinos.FirstOrDefault().FechaHasta)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.Destinos.FirstOrDefault().ComunaDescripcion)
                                        </th>
                                        <th></th>
                                    </tr>
