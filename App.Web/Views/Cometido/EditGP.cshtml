                                    </label>
                                    <div class="section"></div>
                                </div>
                                <div id="auto">
                                    <div class="input-field col s12 m6 l10">
                                        @Html.DropDownListFor(m => Model.TipoVehiculoId, null, "Seleccione...")
                                        @Html.LabelFor(m => Model.TipoVehiculoId)
                                        @Html.ValidationMessageFor(m => Model.TipoVehiculoId, "", new { @class = "red-text" })
                                    </div>
                                    <div class="input-field col s12 m2 l10">
                                        @Html.TextBoxFor(m => Model.PlacaVehiculo)
                                        @Html.LabelFor(m => Model.PlacaVehiculo)
                                        @Html.ValidationMessageFor(m => Model.PlacaVehiculo, "", new { @class = "red-text" })
                                    </div>
                                    <div class="input-field col s12 m2 l10">
                                        @Html.EditorFor(m => Model.VehiculoObservaciones)
                                        @Html.LabelFor(m => Model.VehiculoObservaciones)
                                    </div>
                                </div>
                            </div>

                            <h3><span style="font-weight: bold">III. Datos del Cometido: </span></h3>
                            <div class="row">
                                <div class="input-field col s12 m10 l10">
                                    <strong>
                                        @Html.DisplayNameFor(m => Model.NombreCometido)
                                    </strong>
                                    @Html.EditorFor(m => Model.NombreCometido)
                                    @Html.ValidationMessageFor(m => Model.NombreCometido, "", new { @class = "red-text" })
                                </div>
                                <div class="input-field col s12 m10 l10">
                                    <strong>
                                        @Html.DisplayNameFor(m => Model.CometidoDescripcion)
                                    </strong>
                                    @Html.EditorFor(m => Model.CometidoDescripcion)
                                    @Html.ValidationMessageFor(m => Model.CometidoDescripcion, "", new { @class = "red-text" })
                                </div>
                                <div class="input-field col s12 m10 l10">
                                    <span class="card-title">Destinos</span>
                                </div>

                                @*@if (Model.Destinos.Count > 0)*@

