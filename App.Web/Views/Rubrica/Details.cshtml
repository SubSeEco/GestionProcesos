@model App.Model.Core.Rubrica

<h3>Detalle Rúbrica</h3>
<div class="card">
    <div class="card-content">
        <div class="input-field col s12 m10 l10">
            <strong>@Html.DisplayNameFor(m => Model.Email)</strong>
            <p>@Html.DisplayFor(m => Model.Email)</p>
        </div>
        <div class="input-field col s12 m10 l10">
            <strong>@Html.DisplayNameFor(m => Model.IdentificadorFirma)</strong>
            <p>@Html.DisplayFor(m => Model.IdentificadorFirma)</p>
        </div>
        <div class="input-field col s12 m10 l10">
            <strong>@Html.DisplayNameFor(m => Model.IdProceso)</strong>
            <p>@Html.DisplayFor(m => Model.IdProceso)</p>
        </div>
        <div class="input-field col s12 m10 l10">
            <label>
                @Html.CheckBoxFor(m => m.HabilitadoFirma, new { @disabled = "disabled" })
                <span>@Html.LabelFor(m => Model.HabilitadoFirma)</span>
            </label>
        </div>
        @*<div class="input-field col s12 m10">
                <strong>@Html.DisplayNameFor(m => Model.File)</strong><br />
                <img src="data:image;base64,@System.Convert.ToBase64String(Model.File)" />
            </div>*@
        @*<a href='@Url.Action("Delete", new { id = Model.RubricaId })' class="btn red">Eliminar</a>*@
    </div>
</div>
<div class="fixed-action-btn">
    <a href="javascript:history.back()" class="btn-floating btn-large grey"><i class="large material-icons">chevron_left</i></a>
</div>
<script src="~/Scripts/jquery.validate.min.js"></script>
<script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
<script type="text/javascript">
    $(document).ready(function () {
        $('input:hidden').each(function (index, element) {
            $(element).appendTo($(element).parent());
        });
    });
</script>