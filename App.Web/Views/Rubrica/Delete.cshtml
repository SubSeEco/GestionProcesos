@model App.Model.Core.Rubrica

<h3>Eliminar Rúbrica</h3>

<div class="card">
    <div class="card-content">
        <div class="input-field col s12 m10">
            <strong>@Html.DisplayNameFor(m => Model.Email)</strong>
            <p>@Html.DisplayFor(m => Model.Email)</p>
        </div>
        <div class="input-field col s12 m10">
            <strong>@Html.DisplayNameFor(m => Model.IdentificadorFirma)</strong>
            <p>@Html.DisplayFor(m => Model.IdentificadorFirma)</p>
        </div>
        <div class="input-field col s12 m10">
            <strong>@Html.DisplayNameFor(m => Model.IdProceso)</strong>
            <p>@Html.DisplayFor(m => Model.IdProceso)</p>
        </div>
        <div class="input-field col s12 m10">
            <strong>@Html.DisplayNameFor(m => Model.HabilitadoFirma)</strong>
            <p>@Html.DisplayFor(m => Model.HabilitadoFirma)</p>
        </div>
        @*<div class="input-field col s12 m10">
            <strong>@Html.DisplayNameFor(m => Model.File)</strong><br />
            <img src="data:image;base64,@System.Convert.ToBase64String(Model.File)" />
        </div>*@
        <br />
        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()
            <button class="waves-effect waves-light btn red" type="submit">Confirmar eliminación</button>
        }

    </div>
</div>
<div class="fixed-action-btn">
    <a href="javascript:history.back()" class="btn-floating btn-large grey"><i class="large material-icons">chevron_left</i></a>
</div>
