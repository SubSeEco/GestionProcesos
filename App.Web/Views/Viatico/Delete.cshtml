@model App.Model.Cometido.Viatico

<h3>Eliminar Viatico</h3>

<div class="card">
    <div class="card-content">
        @*<span class="card-title">Confirmar eliminación</span>*@
        <div class="row">
            <div class="input-field col s12 m10">
                <strong>@Html.DisplayNameFor(m => Model.Rango1)</strong>
                <p>@Html.DisplayFor(m => Model.Rango1)</p>
            </div>
            <div class="input-field col s12 m10">
                <strong>@Html.DisplayNameFor(m => Model.Rango2)</strong>
                <p>@Html.DisplayFor(m => Model.Rango2)</p>
            </div>
            <div class="input-field col s12 m10">
                <strong>@Html.DisplayNameFor(m => Model.Rango3)</strong>
                <p>@Html.DisplayFor(m => Model.Rango3)</p>
            </div>
            <div class="input-field col s12 m10">
                <strong>@Html.DisplayNameFor(m => Model.Rango4)</strong>
                <p>@Html.DisplayFor(m => Model.Rango4)</p>
            </div>
            <div class="input-field col s12 m10">
                <strong>@Html.DisplayNameFor(m => Model.Rango5)</strong>
                <p>@Html.DisplayFor(m => Model.Rango5)</p>
            </div>
            <div class="input-field col s12 m10">
                <strong>@Html.DisplayNameFor(m => Model.Año)</strong>
                <p>@Html.DisplayFor(m => Model.Año)</p>
            </div>
        </div>
        <br />
        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()
            @Html.HiddenFor(q => Model.ViaticoId)

            <button class="waves-effect waves-light btn red" type="submit">Confirmar eliminación</button>
        }
    </div>
</div>
<div class="fixed-action-btn">
    <a href="javascript:history.back()" class="btn-floating btn-large grey"><i class="large material-icons">chevron_left</i></a>
</div>
