@model App.Model.Core.Workflow
<div class="card">
    <div class="card-content">
        <span class="card-title">Detalles</span>
        <div class="row">
            <div class="input-field col s12 m10">
                <strong>@Html.DisplayNameFor(m => Model.WorkflowId)</strong>
                <p>@Html.DisplayFor(m => Model.WorkflowId)</p>
            </div>
            <div class="input-field col s12 m10">
                <strong>@Html.DisplayNameFor(m => Model.DefinicionWorkflow.Nombre)</strong>
                <p>@Html.DisplayFor(m => Model.DefinicionWorkflow.Nombre)</p>
            </div>
            <div class="input-field col s12 m10">
                <strong>@Html.DisplayNameFor(m => Model.FechaCreacion)</strong>
                <p>@Html.DisplayFor(m => Model.FechaCreacion)</p>
            </div>
            <div class="input-field col s12 m10">
                <strong>@Html.DisplayNameFor(m => Model.FechaTermino)</strong>
                <p>@Html.DisplayFor(m => Model.FechaTermino)</p>
            </div>
        </div>
        <br />
        <a href='@Url.Action("Edit", new { id = Model.WorkflowId })' class="btn blue">Editar</a>
        <a href='@Url.Action("Delete", new { id = Model.WorkflowId })' class="btn red">Eliminar</a>

    </div>
</div>
<div class="fixed-action-btn">
    <a href="javascript:history.back()" class="btn-floating btn-large grey"><i class="large material-icons">chevron_left</i></a>
</div>