@model IEnumerable<App.Model.Pasajes.Pasaje>

<h3>Pasajes</h3>
<div class="card">
    <div class="card-content">
        <table class="table">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.PasajeId)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Nombre)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Rut)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.DV)
                </th>
                <th>
                    @*@Html.DisplayNameFor(model => model.RegionDescripcion)*@
                </th>
                <th></th>
            </tr>
            @foreach (var item in Model)
            {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.PasajeId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Nombre)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Rut)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.DV)
                </td>
                <td>
                    @*@Html.DisplayFor(modelItem => item.RegionDescripcion)*@
                </td>
            </tr>
            }
        </table>
    </div>
</div>

