@model App.Model.Core.Configuracion

<h3>Detalles configuración</h3>

<div class="card">
    <div class="card-content">
        @*<span class="card-title">Detalles</span>*@
        <div class="row">
            <div class="input-field col s12 m10">
                <strong>@Html.DisplayNameFor(m => Model.Nombre)</strong>
                <p>@Html.DisplayFor(m => Model.Nombre)</p>
            </div>
            <div class="input-field col s12 m10">
                <strong>@Html.DisplayNameFor(m => Model.Valor)</strong>
                <p>@Html.DisplayFor(m => Model.Valor)</p>
            </div>
        </div>
        <br />
        <a href='@Url.Action("Edit", new { id = Model.ConfiguracionId })' class="btn blue">Editar</a>

    </div>
</div>
<div class="fixed-action-btn">
    <a href="javascript:history.back()" class="btn-floating btn-large grey"><i class="large material-icons">chevron_left</i></a>
</div>