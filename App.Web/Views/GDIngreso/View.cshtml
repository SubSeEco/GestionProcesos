@model App.Model.GestionDocumental.GDIngreso

<h3>Detalles</h3>


<div class="row">
    <div class="col s12 m6 l3">
        <div class="card">
            <div class="card-content">
                <span class="card-title">Ingreso GD</span>
                <div class="row">

                    <div class="input-field col s12 m10">
                        <strong>@Html.DisplayNameFor(m => Model.GDTipoIngresoId)</strong><br />
                        @Html.DisplayFor(m => Model.GDTipoIngresoId)
                    </div>
                    <div class="input-field col s12 m10">
                        <strong>@Html.DisplayNameFor(m => Model.Asunto)</strong><br />
                        @Html.DisplayFor(m => Model.Asunto)
                    </div>
                    <div class="input-field col s12 m10">
                        <strong>@Html.DisplayNameFor(m => Model.Referencia)</strong><br />
                        @Html.DisplayFor(m => Model.Referencia)
                    </div>
                    <div class="input-field col s12 m10">
                        <strong>@Html.DisplayNameFor(m => Model.Descripcion)</strong><br />
                        @Html.DisplayFor(m => Model.Descripcion)
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col s12 m6 l3">
        @Html.Action("Documents", "Workflow", new { Model.WorkflowId })
        @Html.Action("History", "Workflow", new { Model.WorkflowId })
    </div>
</div>