@model App.Core.Entities.CuentaRed.CuentaRed

@Html.Action("Header", "Workflow", new { Model.WorkflowId })

<div class="row">
    <div class="col s12 m6 l12">
        @Html.Action("Validate", "Workflow", new { id = Model.WorkflowId })
    </div>

    <div class="col s12 m6 l6">
        <div class="card">
            <div class="card-content">
                <span class="card-title">Cuenta de red</span>

                <div class="row">
                    <div class="input-field col s12 m10">
                        <strong>@Html.DisplayNameFor(m => Model.Solicitante)</strong><br />
                        @Html.DisplayFor(m => Model.Solicitante)
                    </div>
                    <div class="input-field col s12 m10">
                        <strong>@Html.DisplayNameFor(m => Model.ApellidoParterno)</strong><br />
                        @Html.DisplayFor(m => Model.ApellidoParterno)
                    </div>
                    <div class="input-field col s12 m10">
                        <strong>@Html.DisplayNameFor(m => Model.ApellidoMaterno)</strong><br />
                        @Html.DisplayFor(m => Model.ApellidoMaterno)
                    </div>
                    <div class="input-field col s12 m10">
                        <strong>@Html.DisplayNameFor(m => Model.Nombres)</strong><br />
                        @Html.DisplayFor(m => Model.Nombres)
                    </div>
                    <div class="input-field col s12 m10">
                        <strong>@Html.DisplayNameFor(m => Model.RUT)</strong><br />
                        @Html.DisplayFor(m => Model.RUT)
                    </div>
                    <div class="input-field col s12 m10">
                        <strong>@Html.DisplayNameFor(m => Model.FechaNacimiento)</strong><br />
                        @Html.DisplayFor(m => Model.FechaNacimiento)
                    </div>
                    <div class="input-field col s12 m10">
                        <strong>@Html.DisplayNameFor(m => Model.Genero.Nombre)</strong><br />
                        @Html.DisplayFor(m => Model.Genero.Nombre)
                    </div>
                    <div class="input-field col s12 m10">
                        <strong>@Html.DisplayNameFor(m => Model.Pl_UndCod)</strong><br />
                        @Html.DisplayFor(m => Model.Pl_UndDes)
                    </div>
                    <div class="input-field col s12 m10">
                        <strong>@Html.DisplayNameFor(m => Model.Pl_CodCar)</strong><br />
                        @Html.DisplayFor(m => Model.Pl_DesCar)
                    </div>
                    <div class="input-field col s12 m10">
                        <strong>@Html.DisplayNameFor(m => Model.FechaIngreso)</strong><br />
                        @Html.DisplayFor(m => Model.FechaIngreso)
                    </div>
                    <div class="input-field col s12 m10">
                        <strong>@Html.DisplayNameFor(m => Model.RequierePC)</strong><br />
                        @(Model.RequierePC ? "Si" : "No")
                    </div>
                    <div class="input-field col s12 m10">
                        <strong>@Html.DisplayNameFor(m => Model.NumeroTelefono)</strong><br />
                        @Html.DisplayFor(m => Model.NumeroTelefono)
                    </div>
                    <div class="input-field col s12 m10">
                        <strong>@Html.DisplayNameFor(m => Model.NumeroAnexo)</strong><br />
                        @Html.DisplayFor(m => Model.NumeroAnexo)
                    </div>
                    <div class="input-field col s12 m10">
                        <strong>@Html.DisplayNameFor(m => Model.DependenciaAdministrativa)</strong><br />
                        @Html.DisplayFor(m => Model.DependenciaAdministrativa)
                    </div>
                    <div class="input-field col s12 m10">
                        <strong>@Html.DisplayNameFor(m => Model.UbicacionFisica)</strong><br />
                        @Html.DisplayFor(m => Model.UbicacionFisica)
                    </div>
                    <div class="input-field col s12 m10">
                        <strong>@Html.DisplayNameFor(m => Model.Region.Nombre)</strong><br />
                        @Html.DisplayFor(m => Model.Region.Nombre)
                    </div>
                    <div class="input-field col s12 m10">
                        <strong>@Html.DisplayNameFor(m => Model.Email)</strong><br />
                        @Html.DisplayFor(m => Model.Email)
                    </div>
                    <div class="input-field col s12 m10">
                        <strong>@Html.DisplayNameFor(m => Model.SecretariaAsistente)</strong><br />
                        @Html.DisplayFor(m => Model.SecretariaAsistente)
                    </div>
                    <div class="input-field col s12 m10">
                        <strong>@Html.DisplayNameFor(m => Model.SuperiorDirecto)</strong><br />
                        @Html.DisplayFor(m => Model.SuperiorDirecto)
                    </div>
                    <div class="input-field col s12 m10">
                        <strong>@Html.DisplayNameFor(m => Model.Autorizador)</strong><br />
                        @Html.DisplayFor(m => Model.Autorizador)
                    </div>
                    <div class="input-field col s12 m10">
                        <strong>@Html.DisplayNameFor(m => Model.Observacion)</strong><br />
                        @Html.DisplayFor(m => Model.Observacion)
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col s12 m6 l6">
        @Html.Action("Documents", "Workflow", new { Model.WorkflowId })
        @Html.Action("History", "Workflow", new { Model.WorkflowId })
    </div>
</div>

<div class="fixed-action-btn">
    <a href="#modalCreate" class="btn-floating modal-trigger btn-large blue"><i class="large material-icons">attach_file</i></a>
</div>
